clc; clear all; close all;
f=zeros(30,30); 
f(5:24,13:17)=1;
imshow(f,'InitialMagnification','fit');
 F=fft2(f);
 F2=abs(F); 
figure(1);imshow(F2,[],'InitialMagnification','fit'); 
F=fft2(f, 256, 256);
F2=abs(F); 
figure(2); imshow(F2, []);
 F2=fftshift(F); F2=abs(F2); figure(3);imshow(F2,[]);
F2=log(1+F2);
figure(4); imshow(F2,[]);
f=zeros(30,30); f(5:24,13:17)=1;
F=fft2(f,256,256); F2=fftshift(F); figure(5); imshow(log(1+abs(F2)),[]);
